{
  "name": "jon-postgres",
  "version": "0.0.1",
  "description": "A PostgreSQL client.",
  "author": "Jon Rubins <jonrubins@gmail.com>",
  "scripts": {
    "build":
      "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "precommit": "lint-staged",
    "start": "git pull && yarn start:merge",
    "start:merge": "yarn && yarn start:offline",
    "start:debug": "DEBUG=true && yarn start",
    "start:offline":
      "webpack-dev-server --progress --config webpack.config.dev.js"
  },
  "engines": {
    "node": "8.6.0",
    "npm": "5.3.0",
    "yarn": "1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/jon-postgres.git"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --cache --ext=.js,.jsx",
      "git add"
    ],
    "*.{json,css,md}": ["prettier --write", "git add"],
    "*.{scss}": ["sass-lint -v -q"]
  },
  "dependencies": {
    "@jrubins/react-components": "0.0.3",
    "classnames": "2.2.5",
    "lodash": "4.17.5",
    "moment": "2.21.0",
    "normalize.css": "8.0.0",
    "numeral": "2.0.6",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.5.0",
    "css-loader": "0.28.10",
    "dotenv": "5.0.1",
    "eslint": "4.18.2",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.1",
    "prettier": "1.11.1",
    "react-hot-loader": "4.0.0",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-dotenv-plugin": "2.0.2"
  }
}
